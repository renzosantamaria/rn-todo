{
  "expo": {
    "name": "todo-typescript",
    "slug": "todo-typescript",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dog.jpeg",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.renzo.santamaria.todotypescript"
    },
    "web": {
      "favicon": "./assets/dog.jpeg"
    }
  }
}
